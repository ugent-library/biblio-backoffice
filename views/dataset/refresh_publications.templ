package dataset

import (
	"github.com/ugent-library/biblio-backoffice/ctx"
	"github.com/ugent-library/biblio-backoffice/models"
)

templ RefreshPublications(c *ctx.Ctx, dataset *models.Dataset, relatedPublications []*models.Publication) {
	<div hx-swap-oob="innerHTML:#modals"></div>
	<div hx-swap-oob="innerHTML:#publications-body">
		@publicationsBody(c, dataset, relatedPublications)
	</div>
}
